      "source": [
        "# 消費税を加えた計算結果を返す関数\n",
        "def calculate_tax(price: int, tax: int) -> float:\n",
        "  return price + price * tax / 100\n",
        "\n",
        "total = calculate_tax(110, 10)\n",
        "print (f\"商品購入時の消費税は、{total}円\")\n"
      ]
